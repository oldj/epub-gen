{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "plugins": [
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "test",
    "dist"
  ],
  "externals": {
  }
}
